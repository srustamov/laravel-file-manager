{
    "name": "srustamov/laravel-file-manager",
    "description": "Laravel file manager",
    "type": "library",
    "keywords": ["laravel", "file manager", "vue file manager","file browser"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samir Rustamov",
            "email": "rustemovv96@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-zip": "*"
    },
    "require-dev": {
       "orchestra/testbench": "~4.0",
       "phpunit/phpunit": "~8.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Srustamov\\FileManager\\": "src/",
            "Srustamov\\FileManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
       "test": "vendor/bin/phpunit",
       "test-coverage": "vendor/bin/phpunit --coverage-text"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Srustamov\\FileManager\\FileManagerServiceProvider"
            ]
        }
    }
}
