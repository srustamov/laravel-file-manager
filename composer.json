{
    "name": "srustamov/laravel-file-manager",
    "description": "Laravel file manager",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samir Rustamov",
            "email": "rustemovv96@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-zip": "*",
        "symfony/process": "^5.0",
        "illuminate/console": "^6.0|^7.0"
        "illuminate/config": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/events": "^6.0|^7.0",
        "illuminate/filesystem": "^6.0|^7.0",
        "illuminate/routing": "^6.0|^7.0",
        "illuminate/view": "^6.0|^7.0",
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Srustamov\\FileManager\\": "src/",
            "Srustamov\\FileManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Srustamov\\FileManager\\FileManagerServiceProvider"
            ]
        }
    }
}
